{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "home.html",
      "use": "@vercel/static"
    },
    {
      "src": "public/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://easyticket-backend.onrender.com/"
    },
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/home.html",
      "dest": "/home.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
